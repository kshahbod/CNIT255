package project;

/**
 *
 * @author kaevan
 */




public class Guest extends Person{
    
    String numberGuests;
    String accomodations;
    
    public Guest(String numberGuests, String accomodations, String gender, String name, String age) {
        super(gender, name, age);
        this.numberGuests = numberGuests;
        this.accomodations = accomodations;
    }

    public String getNumberGuests() {
        return numberGuests;
    }

    public void setNumberGuests(String numberGuests) {
        this.numberGuests = numberGuests;
    }

    public String getAccomodations() {
        return accomodations;
    }

    public void setAccomodations(String accomodations) {
        this.accomodations = accomodations;
    }

}
