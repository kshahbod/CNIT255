    }                                    

    private void SaveButtonActionPerformed(java.awt.event.ActionEvent evt) {                                           

        String time = "";
    try {
            time = getCurrentTime("http://api.timezonedb.com/v2/get-time-zone?key=U5M24CM8VUSS&format=json&by=zone&zone=America/Indiana/Indianapolis");
        }
    catch (Exception ex) {
        }
    time = time.substring(274, time.length() - 2);
    
    jTextField1.setText(time);
    }                                          

    private void PriceFieldActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
    }                                          

    private void RandomizeButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                
        s1.setSide("");
        d1.setAlchohol("");
        a1.setAppetizer("");
        de1.setDessert("");
        e1.setEntree("");
        
       
         
        if (AppetizerNo.isSelected()) {
            AppetizerField.setText("None");
        }
        else if (AppetizerYes.isSelected()) {
        AppetizerField.setText(a1.getAppetizer());
        }
        
        if (SidesNo.isSelected()) {
            SidesField.setText("None");
        }
        else if (SidesYes.isSelected()) {
            SidesField.setText(s1.getSide());
        }
        
        if (EntreeNo.isSelected()) {
            EntreeField.setText("None");
        }
        else if (EntreeYes.isSelected()) {
            EntreeField.setText(e1.getEntree());
        }
        
        if (DessertNo.isSelected()) {
            DessertField.setText("None");
        }
        else if (DessertYes.isSelected()) {
            DessertField.setText(de1.getDessert());
        }
        
        if (DrinkNo.isSelected()) {
            DrinkField.setText("None");
        }
        else if (DrinkYes.isSelected()) {
            if (UnderAge.isSelected()) {
            DrinkField.setText("Soft Drinks");
        }
            else {
                DrinkField.setText(d1.getAlchohol());
            }
        }
        
        
       
        
    }                                               

