    private void ClearButtonActionPerformed(java.awt.event.ActionEvent evt) {                                            
        GenderGroup.clearSelection();
        LactoseGroup.clearSelection();
        AppetizerGroup.clearSelection();
        NutGroup.clearSelection();
        GlutenGroup.clearSelection();
        VeganGroup.clearSelection();
        VegetarianGroup.clearSelection();
        SidesGroup.clearSelection();
        EntreeGroup.clearSelection();
        DessertGroup.clearSelection();
        DrinkGroup.clearSelection();
        AgeGroup.clearSelection();
        DrinkField.setText("");
        AppetizerField.setText("");
        SidesField.setText("");
        EntreeField.setText("");
        DessertField.setText("");
        PriceField.setText("");
    }                                           

