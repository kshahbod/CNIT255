    Sides s1 = new Sides("","",false,false,false);
    Drink d1 = new Drink("","",false,false,false);
    Appetizer a1 = new Appetizer("","",false,false,false);
    Dessert de1 = new Dessert("","",false,false,false);
    Entree e1 = new Entree("","",false,false,false);
    Guest g1 = new Guest("", "", "", "", "");
    double sPrice = 0.0;
    double dPrice = 0.0;
    double aPrice = 0.0;
    double ePrice = 0.0;
    double dePrice = 0.0;
    double sum = 0.0;
    String output = "";
    int i = 0;
    String name = "Receipt";
    

    private void RandomizeButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                
        s1.setSide("");
        d1.setDrink("");
        a1.setAppetizer("");
        de1.setDessert("");
        e1.setEntree("");
        g1.setName(NameField.getText());
        g1.setAccomodations(AccomodationField.getText());


        if (AppetizerNo.isSelected()) {
            AppetizerField.setText("N/A");
            PriceFieldA.setText("");
            aPrice = 0.0;

        } else if (AppetizerYes.isSelected()) {
            AppetizerField.setText(a1.getAppetizer());
            PriceFieldA.setText("$" + a1.getPrice());
            aPrice = Double.parseDouble(a1.getPrice());
        }

        if (SidesNo.isSelected()) {
            SidesField.setText("N/A");
            PriceFieldS.setText("");
            sPrice = 0.0;
        } else if (SidesYes.isSelected()) {
            SidesField.setText(s1.getSide());
            PriceFieldS.setText("$" + s1.getPrice());
            sPrice = Double.parseDouble(s1.getPrice());
        }

        if (EntreeNo.isSelected()) {
            EntreeField.setText("N/A");
            PriceFieldE.setText("");
            ePrice = 0.0;
        } else if (EntreeYes.isSelected()) {
            EntreeField.setText(e1.getEntree());
            PriceFieldE.setText("$" + e1.getPrice());
            ePrice = Double.parseDouble(e1.getPrice());
        }

        if (DessertNo.isSelected()) {
            DessertField.setText("N/A");
            PriceFieldD.setText("");
            dePrice = 0.0;
        } else if (DessertYes.isSelected()) {
            DessertField.setText(de1.getDessert());
            PriceFieldDe.setText("$" + de1.getPrice());
            dePrice = Double.parseDouble(de1.getPrice());
        }

        if (DrinkNo.isSelected()) {
            DrinkField.setText("N/A");
            PriceFieldD.setText("");
            dPrice = 0.0;
        } else if (DrinkYes.isSelected()) {
            if (UnderAge.isSelected()) {
                DrinkField.setText("Soft Drinks");
                PriceFieldD.setText("$1.86");
                dPrice = 1.86;

            } else {
                DrinkField.setText(d1.getDrink());
                PriceFieldD.setText("$" + d1.getPrice());
                dPrice = Double.parseDouble(d1.getPrice());
            }
        }
        sum = sPrice + aPrice + ePrice + dPrice + dePrice;
        SumField.setText("$" + Double.toString(sum));


    }                                               

    private void ClearButtonActionPerformed(java.awt.event.ActionEvent evt) {                                            
        GenderGroup.clearSelection();
        LactoseGroup.clearSelection();
        AppetizerGroup.clearSelection();
        NutGroup.clearSelection();
        GlutenGroup.clearSelection();
        VeganGroup.clearSelection();
        VegetarianGroup.clearSelection();
        SidesGroup.clearSelection();
        EntreeGroup.clearSelection();
        DessertGroup.clearSelection();
        DrinkGroup.clearSelection();
        AgeGroup.clearSelection();
        DrinkField.setText("");
        AppetizerField.setText("");
        SidesField.setText("");
        EntreeField.setText("");
        DessertField.setText("");
        PriceFieldD.setText("");
        PriceFieldDe.setText("");
        PriceFieldA.setText("");
        PriceFieldS.setText("");
        PriceFieldE.setText("");
        TimestampField.setText("");
        NameField.setText("");
        AccomodationField.setText("");
        sPrice = 0.0;
        dPrice = 0.0;
        aPrice = 0.0;
        ePrice = 0.0;
        dePrice = 0.0;
        sum = 0.0;
        SumField.setText("");
    }                                           

    private void SaveButtonActionPerformed(java.awt.event.ActionEvent evt) {                                           
        
        //Set timestamp
        String time = "";
        try {
            time = getCurrentTime("http://api.timezonedb.com/v2/get-time-zone?key=U5M24CM8VUSS&format=json&by=zone&zone=America/Indiana/Indianapolis");
        } catch (Exception ex) {
        }
        time = time.substring(274, time.length() - 2);

        TimestampField.setText(time);
        
        
        
        
        //Save to text file
        name = name + (Integer.toString(i));
        
        try (PrintWriter out = new PrintWriter(name)) {
          
            out.println("----------------YOUR RECEIPT------------------");
            out.println(g1.getName());
            out.println(g1.getAccomodations());
            out.println(TimestampField.getText());
            out.println(AppetizerField.getText()+" "+PriceFieldA.getText());
            out.println(SidesField.getText()+" "+PriceFieldS.getText());
            out.println(EntreeField.getText()+" "+PriceFieldE.getText());
            out.println(DrinkField.getText()+" "+PriceFieldD.getText());
            out.println(DessertField.getText()+" "+PriceFieldDe.getText());
            out.println("----------------------------------------------");
            
           
        }
        catch(Exception e){
        System.out.println("Error");
        }
        name = name.substring(0,name.length() - 1);
        i++;
    }                                          

